<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox stylesheets="@css/myOrderProducts.css" alignment="CENTER" prefHeight="515.0" prefWidth="604.0" spacing="20.0"
      xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.github.stilvergp.controller.MyOrderProductsController">
    <children>
        <HBox alignment="CENTER" prefHeight="7.0" prefWidth="564.0">
            <children>
                <Label fx:id="titleLabel" text="Lista de Productos">
                    <font>
                        <Font name="System Bold" size="20.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <TableView fx:id="tableView">
            <columns>
                <TableColumn fx:id="imageColumn" prefWidth="75.0" text="Foto"/>
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Nombre"/>
                <TableColumn fx:id="priceColumn" prefWidth="75.0" text="Precio"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
